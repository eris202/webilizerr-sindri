<div class="clearfix"></div>
<hr class="section-line">
<div class="item py-3" id="{{this.key}}">
    <div class="item-header">
        <div class="item-title">
            <div class="title-sign">
                <div class="sign item-num {{this.passedClass}}">
                    <p>{{this.circleTextDisplay}}</p>
                </div>
                <div class="title">
                    <h3>{{this.friendlyName}}</h3>
                </div>
            </div>
            <div>
                <li class="mx-2 question" data-title="Analytics" data-info="Web analytics let you measure visitor activity on your website. You should have at least one analytics tool installed, 
                    but It can also be good to install a second in order to cross-check the data. Popular Analytics Tools Include: Google Analytics, 
                    Quantcast™, AdobeAnalytics™, Piwik™, chartbeat™, Clicky™, ClickTale™, etc."><a
                        class="hov-c-b d-i-b">
                        What is this?</a></li>
                <div class="item-section">{{this.section}}</div>
                {{!-- <img data-title="Title Here" data-info="Info Here" class="question" src="img/info.png" alt="Info"> --}}
            </div>
        </div>
    </div>
    <div class="item-body myt-2">
        {{#if this.passed}}
        <div class="box-wrapper">
            {{#if this.shortAnswer}}
            <div class="answer">
                <div><img style="height: 50px;" src="img/success.png" alt="Check"></div>
                <div style="margin-top: 20px;">
                    <p>
                        {{this.shortAnswer}}
                    </p>
                </div>
            </div>
            {{/if}}
        </div>
        {{else}}
        {{#if this.shortAnswer}}
        <div class="box-wrapper">
            <div class="answer">
                <div><img style="height: 50px;" src="img/err.png" alt="Check"></div>
                <div style="margin-top: 20px;">
                    <p>
                        {{this.shortAnswer}}
                    </p>
                </div>
            </div>
        </div>
        {{/if}}
        {{#if this.recommendation}}
        <div class="box-wrapper">
            <div class="danger">
                <div><img style="height: 50px;" src="img/info3.png" alt="recommendation"></div>
                <div style="margin-top: 20px;">
                    <p>
                        Recommendation: <br> {{this.recommendation}}
                    </p>
                </div>
            </div>
        </div>
        {{/if}}

        {{/if}}



        {{#if this.data}}
        {{#each this.data}}

        {{#ifObject this}}

        <div class="box-wrapper" style="width: 100%; margin-top: 20px;">
            <div class="info">
                <div><img src="img/info2.png" alt="info"></div>
                {{#ifArray this}}

                {{else ifObject this}}
                <ul>
                    {{#each this}}

                    {{#ifeq @key 'name'}}
                    <li>Name: {{this}}</li>
                    {{/ifeq}}
                    {{/each}}
                </ul>
                {{else}}
                {{@key}} {{this}}
                {{/ifArray}}
            </div>
        </div>

        {{/ifObject}}
        {{/each}}
        {{/if}}
    </div>
</div>